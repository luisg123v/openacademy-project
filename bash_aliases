# A symbolic link in ~/.bash_aliases is pointing to this file

# Aliases to facilitate dealing with odoo parameters
alias odoo='python /home/odoo/odoo/odoo.py -d odoo_curso --addons-path=/home/odoo/my-modules,/home/odoo/odoo/addons'
alias odoo_wdemo='python /home/odoo/odoo/odoo.py -d odoo_curso_wdemo --addons-path=/home/odoo/my-modules,/home/odoo/odoo/addons'

# causes ls to display one result per line
alias ls='ls -1'

cd ~
